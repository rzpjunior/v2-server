{
  "name": "v2-server",
  "description": "dashboard-v2-server",
  "author": "junior <junior@edenfarm.id>",
  "private": true,
  "scripts": {
    "start": "node ./server/server.js",
    "watch-client": "watchify --entry client/src/client.js --outfile client/public/js/bundle.js --verbose ",
    "watch-server": "nodemon server/server.js --watch server --ignore db.json --delay 500ms",
    "watch": "concurrently \"npm run watch-client\" \"npm run watch-server\" --timestamp-format \"HH:mm:ss\" --prefix \"{time} [{name}]\" --kill-others --names \"client,server\" ",
    "build-client": "cross-env NODE_ENV=production browserify client/src/client.js | uglifyjs --compress warnings=false --mangle > client/dist/build.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "~1.18.2",
    "compression": "^1.7.1",
    "connect-redis": "^5.1.0",
    "cookie-parser": "~1.4.3",
    "ejs": "~2.5.7",
    "express": "~4.15.5",
    "express-session": "^1.17.1",
    "lowdb": "^1.0.0",
    "material-design-icons-iconfont": "^6.1.0",
    "moment": "^2.20.1",
    "morgan": "~1.9.0",
    "redis": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "vue": "^2.0.1",
    "vue-axios": "^3.2.4",
    "vue-cli-plugin-vuetify": "^2.1.0",
    "vue-router": "^3.0.1",
    "vuetify": "^2.4.5",
    "vuetify-loader": "^1.7.2",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.0.0-beta.3",
    "vue-chartjs": "^3.5.1",
    "vue-file-agent": "^1.7.3",
    "vue-moment": "^4.1.0",
    "vue-numeric-input": "^1.0.6",
    "vue-toast-notification": "^0.5.4",
    "vue-trend-chart": "^0.15.3",
    "vue-xlsx-table": "^1.2.8",
    "vuetify-datetime-picker": "^2.1.1",
    "vuetify-money": "^1.1.0"
  },
  "devDependencies": {
    "@mdi/font": "^5.1.45",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "concurrently": "^3.5.0",
    "cross-env": "^1.0.6",
    "material-design-icons-iconfont": "^5.0.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "uglify-js": "^2.5.0",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vueify": "^9.1.0",
    "vuetify-loader": "^1.3.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
